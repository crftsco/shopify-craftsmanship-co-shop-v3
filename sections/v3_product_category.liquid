<div class="w-layout-blockcontainer v3--container w-container"><div class="v3--collection-wrapper w-dyn-list"><div role="list" class="v3--grid-products w-dyn-items">{% for product in section.settings.collection_collection.products limit: section.settings.collection_collection_limit %}
<div role="listitem" class="v3--grid-col-product w-dyn-item">
    
    <a li-object:href="product.url" href="{{ product.url }}" class="v3--card-product w-inline-block"><figure class="v3--card-product-figure"><img class="v3--figure-product-image" src="{{ product.featured_image | img_url: 'master' }}" alt="{{ product.featured_image.alt }}" sizes="(max-width: 479px) 87vw, (max-width: 767px) 92vw, (max-width: 991px) 46vw, 30vw" li-object:src="product.featured_image | img_url: 'master'" loading="lazy" li-object:alt="product.featured_image.alt" srcset="{{ '01-p-500.jpg' | asset_url }} 500w, {{ '01-p-800.jpg' | asset_url }} 800w, {{ '01-p-1080.jpg' | asset_url }} 1080w, {{ '01-p-1600.jpg' | asset_url }} 1600w, {{ '01.jpg' | asset_url }} 2000w">
</figure><div class="v3--card-product-content"><div li-object="product.title" class="v3--card-product-title">{{ product.title }}</div><div class="w-layout-hflex v3--card-product-price-wrapper"><div li-object="product.compare_at_price | money" class="v3--card-product-price compare">{{ product.compare_at_price | money }}</div><div li-object="product.price | money" class="v3--card-product-price">{{ product.price | money }}</div></div></div></a>
    
</div>
{% endfor %}
</div><div class="v3--collection-empty w-dyn-empty"><div>No items found.</div></div></div></div> {% schema %} {"tag":"section","name":"V3 Product Category","class":"v3--section","settings":[{"id":"collection_collection","type":"collection","label":"Collection"},{"type":"range","id":"collection_collection_limit","min":1,"max":99,"step":1,"label":"Limit: Collection","default":6}],"presets":[{"name":"V3 Product Category","category":"Liquify","blocks":[]}]} {% endschema %}