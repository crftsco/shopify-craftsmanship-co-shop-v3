      $('html').css('height', '100%');
      $('html').css('position', 'absolute');
      overflowState = 'hidden';
      clicked = 'true';
    } else {
      $('html').css('height', 'auto');
      $('html').css('position', 'relative');
      overflowState = 'unset';
      clicked = 'false';
    }
    $('html, body').css('overflow', overflowState);
  });
}
